{
  "dockerComposeFile": [
    "../docker-compose.yaml",
    "docker-compose.yml"
  ],
  "extensions": [
    "mikestead.dotenv",
    "editorconfig.editorconfig",
    "mrmlnc.vscode-remark",
    "felixfbecker.php-intellisense",
    "felixfbecker.php-debug",
    "ikappas.phpcs"
  ],
  "name": "Development",
  "runServices": [
    "php"
  ],
  "service": "php",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/sh",
    "php.suggest.basic": false
  },
  "workspaceFolder": "/var/www/app",
}
